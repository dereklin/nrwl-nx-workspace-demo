{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "nrwl-nx-workspace-demo",
    "npmScope": "nrwl-nx-workspace-demo",
    "latestMigration": "20180130-angular-devkit-schematics"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/app1/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/app1/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "styleExt": "scss",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  },
  "apps": [
    {
      "name": "app1",
      "root": "apps/app1/src",
      "outDir": "dist/apps/app1",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../../../node_modules/bootstrap/dist/css/bootstrap.css",
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "app-interfaces",
      "root": "libs/app-interfaces/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "app-tokens",
      "root": "libs/app-tokens/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "background-color-randomizer",
      "root": "libs/background-color-randomizer/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "dropdown",
      "root": "libs/dropdown/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "dummy1-service",
      "root": "libs/dummy1-service/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "http-client-service",
      "root": "libs/http-client-service/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "movie-resolver",
      "root": "libs/movie-resolver/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "not-found",
      "root": "libs/not-found/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "session-resolver",
      "root": "libs/session-resolver/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "simple-http-service",
      "root": "libs/simple-http-service/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "stringutils",
      "root": "libs/stringutils/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "tab-nav",
      "root": "libs/tab-nav/src",
      "test": "../../../test.js",
      "appRoot": ""
    }
  ]
}