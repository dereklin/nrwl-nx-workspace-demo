{
  "name": "nrwl-nx-workspace-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng-high-mem": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "start:app1": "npm start -- --hmr --project=app1 --open --port 4501",
    "start:app2": "npm start -- --hmr --project=app2 --open --port 4502",

    "start:app1:ssl": "npm start -- --hmr --project=app1 --open --port 4501 --ssl --ssl-key ./localhost.key  --ssl-cert ./localhost.crt --disable-host-check",

    "app": "ng serve --hmr --open --project",
    "start-app": "node ./scripts/start-app.js ng serve --hmr --open --project",
    "build": "ng build --prod --project=app1",
    "test": "jest",
    "test:karma": "ng test",
    "lint": "./node_modules/.bin/nx lint && npm run affected:lint -- --base=origin/master --head=HEAD --parallel",
    "webdriver-update": "webdriver-manager update",
    "e2e": "ng e2e",
    "e2e:app1": "npm run e2e -- --hmr --app=app1 --webdriver-update false",
    "e2e:app1:quick": "npm run e2e:app1 -- --serve false",
    "e2e:jasmine:app1": "npm run e2e -- --hmr --app=app1 --config ./protractor.conf.jasmine.js --webdriver-update false",
    "build:app1": "npm run build",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "setup": "npm i --no-optional --ignore-scripts",
    "ncu": "ncu",
    "ncu:check": "ncu --reject @types/geojson",
    "ncu:update": "ncu -u --reject @types/geojson",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "start-server-and-test start:app1 http-get://localhost:4200 cypress:run",
    "precommit": "pretty-quick --staged",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dyson": "dyson ./dyson",
    "dyson:watch": "nodemon --watch dyson --exec dyson dyson",
    "help": "./node_modules/.bin/nx help",
    "storybook": "start-storybook -p 6006",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "jest:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "debug-chrome-url": "chrome://inspect/#devices"
  },
