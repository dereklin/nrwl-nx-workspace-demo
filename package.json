  "scripts": {
    "ng": "ng",
    "ng-high-mem": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "start:app1": "npm start -- --hmr --project=app1 --open",
    "build": "ng build",
    "test": "jest",
    "test:karma": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "webdriver-update": "webdriver-manager update",
    "e2e": "ng e2e",
    "e2e:app1": "npm run e2e -- --hmr --app=app1 --webdriver-update false",
    "e2e:app1:quick": "npm run e2e:app1 -- --serve false",
    "e2e:jasmine:app1": "npm run e2e -- --hmr --app=app1 --config ./protractor.conf.jasmine.js --webdriver-update false",
    "build:app1": "npm run build -- --prod --project=app1",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "setup": "npm i --no-optional",
    "ncu": "ncu",
    "ncu:check": "ncu --reject @types/geojson",
    "ncu:update": "ncu -u --reject @types/geojson",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "start-server-and-test start:app1 http-get://localhost:4200 cypress:run",
    "precommit": "pretty-quick --staged",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dyson": "dyson ./dyson",
    "dyson:watch": "nodemon --watch dyson --exec dyson dyson",
    "help": "./node_modules/.bin/nx help",
    "storybook": "start-storybook -p 6006",
    "affected:test": "./node_modules/.bin/nx affected:test"
  },
