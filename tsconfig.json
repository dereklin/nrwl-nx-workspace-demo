{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jasmine"
    ],
    "lib": [
      "esnext",
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@nrwl-nx-workspace-demo/*": [
        "libs/*"
      ],
      "@nrwl-nx-workspace-demo": [
        "libs"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
